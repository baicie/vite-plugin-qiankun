{
  "name": "@baicie/plugin-qiankun",
  "version": "0.1.0",
  "description": "vite for qiankun",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run build-types",
    "build-bundle": "tsup",
    "dev": "rimraf dist && pnpm run build-bundle --watch src",
    "playground:install": "cd playground/main && npm install && cd ../viteapp && npm install && cd ../vue && npm install && cd ../react18 && npm install && cd ../vue3sub && npm install",
    "playground:start": "npm run build && cd playground/viteapp && npm run build && cd ../../ && npm-run-all --parallel start:main start:viteapp start:vue start:react18dev start:vue3sub",
    "playground:start-vite-dev": "npm run build && npm-run-all --parallel start:main start:vitedev start:vue start:react18dev start:vue3sub",
    "start:main": "cd playground/main && npm run start",
    "start:viteapp": "cd playground/viteapp && npm run start",
    "start:vitedev": "cd playground/viteapp && npm run dev",
    "start:react18dev": "cd playground/react18 && npm run dev",
    "start:vue3sub": "cd playground/vue3sub && npm run dev",
    "start:vue": "cd playground/vue && npm run start",
    "vite:test": "npm run build && cd playground/viteapp && npm run dev"
  },
  "keywords": [
    "qiankun",
    "vite",
    "esm",
    "esModule"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "homepage": "https://github.com/baicie/vite-plugin-qiankun",
  "author": "baicie",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@rollup/plugin-typescript": "^11.1.2",
    "@swc/core": "^1.3.71",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.45.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "peerDependencies": {
    "typescript": ">=4",
    "vite": ">=2"
  }
}